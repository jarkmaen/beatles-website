{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "./build/",
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES6"
    }
}
